<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Settings intelligence suggestions class -->
    <string name="config_settingsintelligence_suggestions_class" translatable="false">
        com.android.settings.intelligence.suggestions.SuggestionService
    </string>

    <!-- Whether to show min/max refresh rate in display settings -->
    <bool name="config_show_refresh_rate_controls">false</bool>
    <bool name="config_supports_dynamic_refresh_rate_controls">true</bool>

    <!-- Fingerprint -->
    <bool name="config_is_front_facing_fps" translatable="false">false</bool>

    <!-- Whether to show or hide sensitive info (like IMEI and Phone Number) on About phone -->
    <bool name="configShowDeviceSensitiveInfo">false</bool>
	    
    <!-- Battery Health -->
    <string name="config_batteryCalculatedCapacity">/sys/class/power_supply/bms/charge_full</string>
    <string name="config_batteryDesignCapacity">/sys/class/power_supply/bms/charge_full_design</string>
    <string name="config_batteryChargeCycles">/sys/class/power_supply/bms/cycle_count</string>
    <bool name="config_supportBatteryHealth">false</bool>
	    
    <!-- Max network scan search time in seconds -->
    <integer name="config_network_scan_helper_max_search_time_sec">300</integer>

</resources>
